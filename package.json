{
  "name": "fe_benchmark",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "cd react && npm install && cd .. && cd vue && npm install",
    "build": "npm run build:react && npm run build:vue && npm run copy",
    "build:react": "cd react && npm run build",
    "build:vue": "cd vue && npm run build",
    "del": "del-cli ./build/vue/* && del-cli ./build/react/*",
    "copy": "npm run del && cpr ./vue/dist ./build/vue && cpr ./react/build ./build/react",
    "publish": "gh-pages -d build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suhaotian/FE_Benchmark.git"
  },
